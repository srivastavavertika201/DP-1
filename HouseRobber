class Solution {
    public int rob(int[] nums) {
        int n = nums.length;
        int evenSum =0, oddSum =0;
        for(int i=0;i<n;i=i+2){
            evenSum += nums[i];
        }
        for(int j=1;j<n;j=j+2){
            oddSum += nums[j];
        }
        int max = Math.max(evenSum,oddSum);
        return max;
    }
}

